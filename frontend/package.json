{
    "name": "ng-patient-registry",
    "version": "0.0.1",
    "description": "Frontend layer for Patient Registry",
    "scripts": {
        "start": "npm run dev",
        "dev": "webpack-dev-server --env.browser=yes --env.verbose=no",
        "build": "npm run clean && npm run webpack1 && npm run gzip",
        "clean": "rimraf build && rimraf dist",
        "xwebpack": "npm run webpack1 && npm run webpack2",
        "webpack1": "cross-env webpack --env.environment=production --env.treeshake=yes --env.verbose=no",
        "xwebpack2": "cross-env webpack --env.environment=production --env.treeshake=no --env.basehref=/",
        "gzip": "npm run gzipbuild && npm run gzipext",
        "gzipbuild": "npx gzip build/*.js",
        "gzipext": "npx gzip build/ext/*.js",
        "replace": "npx replace-in-file /.js/g .js.gz build/index.html --isRegex",
        "http": "npx http-server build -g -o",
        "lint": "ng lint",
        "e2e": "ng e2e"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.901.5",
        "@angular/common": "^9.1.7",
        "@angular/compiler": "^9.1.7",
        "@angular/compiler-cli": "^9.1.7",
        "@angular/core": "^9.1.7",
        "@angular/forms": "^9.1.7",
        "@angular/platform-browser": "^9.1.7",
        "@angular/platform-browser-dynamic": "^9.1.7",
        "@angular/router": "^9.1.7",
        "@ngtools/webpack": "~9.1.5",
        "@sencha/ext": "~7.2.0",
        "@sencha/ext-angular-modern": "~7.2.0",
        "@sencha/ext-modern": "~7.2.0",
        "@sencha/ext-modern-theme-material": "~7.2.0",
        "@sencha/ext-modern-treegrid": "~7.2.0",
        "@sencha/ext-ux": "~7.2.0",
        "@sencha/ext-webpack-plugin": "~7.2.0",
        "@types/core-js": "~2.5.3",
        "@types/node": "~13.9.8",
        "@webcomponents/webcomponentsjs": "^2.4.3",
        "base-href-webpack-plugin": "~2.0.0",
        "core-js": "~3.6.5",
        "cpx": "^1.5.0",
        "cross-env": "~7.0.2",
        "css-loader": "~3.4.2",
        "file-loader": "~6.0.0",
        "gzip-cli": "^1.1.1",
        "html-loader": "~1.0.0",
        "html-webpack-plugin": "3.2.0",
        "http-server": "^0.12.3",
        "portfinder": "~1.0.26",
        "replace-in-file": "^5.0.2",
        "rimraf": "~3.0.2",
        "style-loader": "~1.1.4",
        "to-string-loader": "~1.1.6",
        "typescript": "3.8.3",
        "webpack": "~4.42.1",
        "webpack-cli": "~3.3.11",
        "webpack-dev-server": "^3.11.0",
        "webpack-filter-warnings-plugin": "~1.2.1",
        "zone.js": "~0.10.3"
    },
    "dependencies": {},
    "private": false,
    "main": "",
